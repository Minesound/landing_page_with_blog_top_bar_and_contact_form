---
import Layout from '../Base.astro';
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';

const title = 'Thanks!';
const description = "Thanks for contacting me, I'll be in touch shortly.";
const permalink = 'https://stefenphelps.com/success';
---

<Layout title={title} description={description} permalink={permalink}>
	<Header activeNav='contact' />
	<main>
		<article class='wrapper'>
			<h1>{title}</h1>
			<p>{description}</p>
		</article>
	</main>
	<Footer />
</Layout>
